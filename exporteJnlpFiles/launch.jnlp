<?xml version="1.0" encoding="utf-8"?> 
<!-- JNLP File for SecretSanta --> 
<jnlp 
     spec="1.0+" 
     codebase="https://sites.google.com/site/cwinsorpublic/home/java-apps/secret-santa"  
     href="launch.jnlp">	
     <information>	
          <title>SecretSanta</title>	
          <vendor>Chris Winsor</vendor>	
          <homepage href="https://sites.google.com/site/cwinsorpublic/home/java-apps/secret-santa"/>	
          <offline-allowed/>	
          <update check="background" policy="prompt-update"/>	
     </information>	
     <security>
            <!-- NO PERMISSIONS   <all-permissions/> -->
     </security> 
     <resources>	
          <!-- Application Resources -->	
          <j2se version="1.6+"/>	
          <jar href="SecretSanta.jar" main="true"/>	
     </resources>	
     <application-desc	
           name="SecretSanta"
	   main-class="cwinsor.com.SecretSanta"
           width="300"
           height="300">
     </application-desc>	
</jnlp>	
